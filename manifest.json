{
  "manifest_version": 3,
  "name": "Advanced Proxy & Fingerprint Spoofer",
  "version": "2.0",
  "description": "Proxy xoay + Fake fingerprint tự động",
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "declarativeNetRequest",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon16.png"
  }
}